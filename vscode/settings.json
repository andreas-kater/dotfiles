{
  "terminal.integrated.shell.osx": "zsh",
  "window.zoomLevel": 1,
  "explorer.confirmDelete": false,
  "explorer.sortOrder": "mixed",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.rendererType": "dom",
  "html.format.indentInnerHtml": true,
  "html.format.maxPreserveNewLines": 9,
  "html.format.wrapAttributes": "auto",
  "html.format.indentHandlebars": true,
  "brace_style": "collapse,preserve-inline",
  "editor.gotoLocation.multiple": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.suggestSelection": "first",
  "editor.minimap.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // "eslint.options": {
  //   "configFile": "/Users/andreaskater/.eslintrc"
  // },
  "eslint.codeAction.showDocumentation": {},
  "eslint.format.enable": true,
  "files.exclude": {
    "**/*.pyc": true,
    "**/__pycache__": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/build": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vim.leader": ",",
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "k",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "Tab"
      ],
      "after": [
        ">"
      ]
    },
    {
      "before": [
        "S-Tab"
      ],
      "after": [
        "<"
      ]
    },
    {
      "before": [
        ",",
        "s"
      ],
      "commands": [
        ":sort"
      ]
    }
  ],
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "breadcrumbs.enabled": false,
  "vscodeGoogleTranslate.preferredLanguage": "fr",
  "[javascript]": {
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 4
  },
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "ruby.intellisense": "rubyLocate",
  "references.preferredLocation": "view",
  "workbench.tree.indent": 30,
  "workbench.colorTheme": "Material Theme Ocean",
  "python.jediEnabled": true,
  "python.languageServer": "Jedi",
  "python.sortImports.args": [
    "--check-only"
  ],
  "pyhthon.formatting.autopep8Args": [
    "--ignore",
    "E1101",
    "C0111",
    "E402",
    "E301",
    "E501",
    "E266"
  ],
  "python.linting.pylintArgs": [
    "--disable=E1101,C0111,E402,E301,E501,E266",
    // "--errors-only",
    "--load-plugins"
    // "pylint_flask"
  ],
  "python.linting.flake8Args": [
    "--ignore=E1101,C0111,E402,E301,E501,E266",
    "--verbose"
  ],
  "python.pythonPath": "${workspaceFolder}/venv/bin/python",
  "python.linting.pycodestyleArgs": [
    "--ignore=E1101,C0111,E402,E301,E501,E266"
  ],
  "eslint.lintTask.enable": true,
  "editor.renameOnType": true,
  "beautify.language": {
    "html": ["html", "php", "erb"],
    "css": ["scss","css"],
    "js": []
  }
}